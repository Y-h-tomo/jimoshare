= form_with model: @ticket, url: item_tickets_path, class: 'item-form level', remote: true do |f|
  .container
    .field
      = render 'shared/error_messages', model: f.object
    .field.control.has-icons-left.hero
      span.icon
        i.fas.fa-ticket-alt.has-text-danger.is-large
      = f.submit "チケット購入する"